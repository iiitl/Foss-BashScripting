#!/usr/bin/env bash

# Remove all the binary files (usually made after gcc) present inside current directory
# Refer "man find" for more information, you can also use modern tool like fd instead of find
for file in *; do 
    if [[ -f $file ]]; then 
        if file $file | grep -q "ELF"; then 
            rm -f $file 
        fi 
    fi 
done